local Settings = {
    rewrittenmain = {
        Enabled = true,
        Key = "q", -- // Key to enable / disable
        DOT = true, -- // Keep on
        AIRSHOT = true, -- // Airshot func
        NOTIF = true, -- // Wether to show notifications or not i would not turn off
